/*公共样式start*/
/*body,h1,h2,h3,h4,h5,h6,p,div,article,textarea{
	line-height: 1em;
}*/
body {
	color: #303030;
	font-size: 15px;
	line-height: normal;
	background-color: #f8f8f8;
	margin: 0;
	padding: 0;
}
svg.icon{
	width: 16px;
	height: 16px;
}

ul{
	list-style: none;
	margin: 0;
	padding: 0;
}
h3,h2{
	margin: 0;
	padding: 0;
	font-weight: normal;
}
p {
	font-size: 13px;
	color: #bbbbbb;
}
.height-13{
	height: 13px ;
	background-color: #f8f8f8;
}
.height-100{
	height: 100px ;
	background-color: white;
}
.height-300{
	height: 300px;
	background-color: white;
}
.clear:after{
　　　　　　content:"";//设置内容为空
　　　　　　height:0;//高度为0
　　　　　　line-height:0;//行高为0
　　　　　　display:block;//将文本转为块级元素
　　　　　　visibility:hidden;//将元素隐藏
　　　　　　clear:both//清除浮动
}
.radius-3-lb{
	border-radius: 500px;
	border-bottom-left-radius: 0;
}
.radius-3-rb{
	border-radius: 500px;
	border-bottom-right-radius: 0;
}
.radius-2-l{
	border-bottom-left-radius: 500px;
	border-top-left-radius: 500px;
}
.radius-2-r{
	border-bottom-right-radius: 500px;
	border-top-right-radius: 500px;
}
.radius-2-lb{
	border-top-left-radius: 8px;
	border-bottom-right-radius:8px;
}
.radius-2-rb{
	border-top-right-radius: 10px;
	border-bottom-left-radius: 10px;
}
.mui-table-view:before,
.mui-table-view:after {
	height: 0;
}
.mui-content button:disabled{
	background-color: #dddddd;
	opacity: 1;
	border: none;
}
/*toast信息提示*/
.mui-toast-container {
	bottom: 35% !important;
}
/*查看更多*/
.more-btn{
