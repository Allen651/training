/*规则弹窗*/
/*音符 音乐*/
#musicicon{
	position: fixed;
	top: 54px;
	right: 15px;
	z-index: 8;
	border-radius: 66px;
	overflow: hidden;
	animation: myrotate 3s infinite linear;
	-webkit-animation: myrotate 3s infinite linear;
	border: 1px solid white;
	background-color: rgba(0, 255, 255, 0.3);
	width: 32px;
	height: 32px;
	padding-left: 3px;
}			
@keyframes myrotate {
	0% {
		transform: rotateZ(0deg);
		/*rotateZ(0deg)*/
		-ms-transform: rotateZ(0deg);
		-webkit-transform: rotateZ(0deg);
	}
	50% {
		transform: rotateZ(180deg);
		-ms-transform: rotateZ(180deg);
		-webkit-transform: rotateZ(180deg);
	}
	100% {
		transform: rotateZ(360deg);
		-ms-transform: rotateZ(360deg);
		-webkit-transform: rotateZ(360deg);
	}
}

@keyframes myup {
	0% {
		transform: translateY(0) rotateZ(-45deg);
		-ms-transform: translateY(0) rotateZ(-45deg);
		-webkit-transform: translateY(0) rotateZ(-45deg);
	}
	50% {
		transform: translateY(-10px) rotateZ(-45deg);
		-ms-transform: translateY(-10px) rotateZ(-45deg);
		-webkit-transform: translateY(-10px) rotateZ(-45deg);
		opacity: 0.5;
	}
	100% {
		transform: translateY(-20px) rotateZ(-45deg) perspective(0);
		-ms-transform: translateY(-20px) rotateZ(-45deg) perspective(0);
		-webkit-transform: translateY(-20px) rotateZ(-45deg) perspective(0);
		opacity: 0;
	}
}
