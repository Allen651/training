}
.more-btn:active{
	background-color: #eeeeee;
}

.my-foot-tab {
	height: 49px;
	position: fixed;
	bottom: 0;
	left: 0;
	background-color: white;
	border-top: 0.5px solid #dddddd;
	width: 100%;
	margin: 0;
	z-index: 99;
}

.my-foot-tab .my-tab-item {
	display: inline-block;
	text-align: center;
	position: relative;
	top: 0;
	left: 0;
	padding: 4px 0;
	height: 49px;
}

.my-foot-tab .my-tab-item .mui-badge {
	width: 10px;
	height: 10px;
	font-size: 10px;
	position: absolute;
	top: 4px;
	left: 53%;
	padding: 2px;
}

.errormessage #error-message .icon {
	margin-right: 5px;
	font-size: 13px;
	color: red;
}

.my-foot-tab .my-tab-item .icon {
	width: 26px;
	height: 26px;
	font-size: 26px;
	color: #303030;
	display: block;
	margin: auto;
	margin-bottom: 5px;
}

.my-foot-tab .my-tab-item svg.icon {
	margin-top: 1px;
	margin-bottom: 4px;
}

.my-foot-tab .my-tab-item .my-tab-label {
	display: block;
	font-size: 10px;
	color: #303030;
	line-height: 10px;
}

.my-foot-tab .xian {
	width: 0.5px;
	height: 26px;
	background-color: #dddddd;
	margin-top: -34px;
}

.my-foot-tab .btn {
	background-color: rgb(255, 218, 0);
}

.mui-bar-nav{
	-webkit-box-shadow: none;
	box-shadow: none;
}
.touxiang{
	width: 100%;
	position: relative;
	top: 0;
	left: 0;
	padding-top:3px ;
}
 .toptab .flexbox{
