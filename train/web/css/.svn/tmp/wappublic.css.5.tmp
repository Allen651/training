/*清除格式*/

body {
	color: #303030;
	font-size: 15px;
}

.mui-table-view:before,
.mui-table-view:after {
	height: 0;
}

.mui-title {
	margin: 0;
	line-height: 40px;
}
.mui-content{
	padding-bottom: 50px;
}
.mui-content>div:first-child {
	padding: 8px 15px;
	border-bottom: 0.5px solid #dddddd;
	background-color: white;
	font-size: 15px;
	color: #303030;
	max-height: 40.5px;
}

.mui-content>div:first-child>span:first-child {
	color: #999999;
	font-size: 14px;
	margin-top: 2px;
	margin-left: -6px;
}
.my-foot-tab {
	height: 49px;
	position: fixed;
	bottom: 0;
	left: 0;
	background-color: white;
	border-top: 0.5px solid #dddddd;
	width: 100%;
	margin: 0;
	z-index: 99;
}

.my-foot-tab .my-tab-item {
	display: inline-block;
	text-align: center;
	position: relative;
	top: 0;
	left: 0;
	padding: 4px 0;	
	height: 49px;
}

.my-foot-tab .my-tab-item .mui-badge {
	width: 14px;
	height: 14px;
	font-size: 10px;
	position: absolute;
	top: 0;
	left: 58%;
	padding: 2px;
}

.my-foot-tab .my-tab-item .icon {
	width: 26px;
	height: 26px;
	font-size: 26px;
	color: #303030;
	display: block;
    margin: auto;
    margin-bottom: 5px;
}

.my-foot-tab .my-tab-item .my-tab-label {
	display: block;
	font-size: 10px;
	color: #303030;
	line-height: 10px;
}
.my-foot-tab .xian{
	width:0.5px;
	height: 26px;
	background-color: #dddddd;
	margin-top: -34px;	
}
.my-foot-tab .btn{
	background-color: rgb(255,218,0);
}
.mui-bar {
	-webkit-box-shadow: 0 0 1px #dddddd;
	box-shadow: 0 0 1px #dddddd;
}


/*规定字体*/

html,
body,
div,
form,
ul,
table,
input,
select,
textarea,
button {
	font-family: "Hiragino Sans GB W3","microsoft yahei";
}

p {
	font-size: 13px;
	color: #bbbbbb;
}
