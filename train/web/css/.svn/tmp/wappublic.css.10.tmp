/*清除格式*/

/*初始化*/
/*公共样式start*/
body {
	color: #303030;
	font-size: 15px;
	line-height: normal;
	background-color: #f8f8f8;
	margin: 0;
	padding: 0;
}
svg.icon{
	width: 16px;
	height: 16px;
}

ul{
	list-style: none;
	margin: 0;
	padding: 0;
}
h3,h2{
	margin: 0;
	padding: 0;
	font-weight: normal;
}
p {
	font-size: 13px;
	color: #bbbbbb;
}
.height-13{
	height: 13px ;
	background-color: #f8f8f8;
}
.height-100{
	height: 100px ;
	background-color: white;
}
.height-300{
	height: 300px;
	background-color: white;
}
.clear:after{
　　　　　　content:"";//设置内容为空
　　　　　　height:0;//高度为0
　　　　　　line-height:0;//行高为0
　　　　　　display:block;//将文本转为块级元素
　　　　　　visibility:hidden;//将元素隐藏
　　　　　　clear:both//清除浮动
}
.mui-table-view:before,
.mui-table-view:after {
	height: 0;
}
.mui-title {
	margin: 0;
	line-height: 40px;
}
.mui-content {
	padding-bottom: 50px;
	background-color: #f8f8f8;
}

.mui-content>div:first-child {
	padding: 10px 15px;
	border-bottom: 0.5px solid #dddddd;
	background-color: white;
	font-size: 15px;
	color: #303030;
	max-height: 40.5px;
}

.mui-content>div:first-child>span:first-child {
	color: #999999;
	font-size: 14px;
	margin-top: 2px;
}
.mui-content button:disabled{
	background-color: #dddddd;
	opacity: 1;
	border: none;
}
.mui-content .more-btn:active{
	background-color: #eeeeee;
}

.my-foot-tab {
	height: 49px;
	position: fixed;
	bottom: 0;
	left: 0;
	background-color: white;
	border-top: 0.5px solid #dddddd;
	width: 100%;
	margin: 0;
	z-index: 99;
}

.my-foot-tab .my-tab-item {
	display: inline-block;
	text-align: center;
	position: relative;
	top: 0;
	left: 0;
	padding: 4px 0;
	height: 49px;
}

.my-foot-tab .my-tab-item .mui-badge {
	width: 10px;
	height: 10px;
	font-size: 10px;
	position: absolute;
	top: 4px;
	left: 53%;
	padding: 2px;
}

.errormessage #error-message .icon {
	margin-right: 5px;
	font-size: 13px;
	color: red;
}

.my-foot-tab .my-tab-item .icon {
	width: 26px;
	height: 26px;
	font-size: 26px;
	color: #303030;
	display: block;
	margin: auto;
	margin-bottom: 5px;
}

.my-foot-tab .my-tab-item svg.icon {
	margin-top: 1px;
	margin-bottom: 4px;
}

.my-foot-tab .my-tab-item .my-tab-label {
	display: block;
	font-size: 10px;
	color: #303030;
	line-height: 10px;
}

.my-foot-tab .xian {
	width: 0.5px;
	height: 26px;
	background-color: #dddddd;
	margin-top: -34px;
}

.my-foot-tab .btn {
	background-color: rgb(255, 218, 0);
}

.mui-bar {
	-webkit-box-shadow: 0 0 1px #dddddd;
	box-shadow: 0 0 1px #dddddd;
}
.touxiang{
	width: 100%;
	position: relative;
	top: 0;
	left: 0;
	padding-top:3px ;
}
/*公共样式end*/


