/*主播信息表*/
.zhuboxinxi .zhubo-form{
	background-color: white;
}
.zhuboxinxi .zhubo-form select{
	margin: 0;
	padding: 0;
	line-height: 22px;
}
.zhuboxinxi .zhubo-form select{
	width:100% ;
	direction: rtl;
}
.zhuboxinxi .zhubo-form select option{
	direction: rtl;
}
.zhuboxinxi .zhubo-form .zbpt,.zhubo-form .fensi{
	display: none;
}
.zhuboxinxi .zhubo-form input{
	border: none;
	text-align: right;
	font-size: 14px;
}
.zhuboxinxi .zhubo-form ul{
	margin: 0px;
	padding: 0px;
}
.zhuboxinxi .zhubo-form li{
	padding:14px 27px; /*//有条通线不能设置ul padding*/
	border-bottom: 0.5px solid #f2f2f2;
	list-style: none;
}
.zhuboxinxi .zhubo-form>div:last-child{
	background-color:rgb(248,248,248);
	height: 90px;
	padding: 22px;
	position: relative;
	top: 0;
	left: 0;
	text-align: center;
	padding-top: 32px;
}
.zhuboxinxi .zhubo-form li input:-ms-input-placeholder{
	font-size: 14px;
}
.zhuboxinxi .zhubo-form li input::-webkit-input-placeholder{
	font-size: 14px;
}
.zhuboxinxi .zhubo-form li input::-moz-placeholder{
	font-size: 14px;
}
.zhuboxinxi .zhubo-form li input[type="radio"]:first-child+span{
	margin-right: 26px;
}
.zhuboxinxi .zhubo-form li .pingtai input[type="radio"]:first-child+span{
	margin-right: 0;
}
.zhuboxinxi .zhubo-form div button{
	color: white;
	background-color: rgb(48,48,48);
	width:85%;
	border-radius: 20px;
	padding: 8px;
	font-size: 18px;
}
.zhuboxinxi .zhubo-form li>div{
	float: right;
}
.zhuboxinxi .zhubo-form input[type=date]{
	margin: 0;
	padding: 0;
	line-height: 22px;
	height: 22px;
	direction: rtl;
	width: 82%;
	right: -20px;
}
.zhuboxinxi .zhubo-form input[type="radio"]{
	width: 18px;
	height: 18px;
	vertical-align: -3px;
	margin-right: 5px;
	margin-bottom: 6px;
}
.zhuboxinxi .zhubo-form input[type="radio"]+span{
	color: rgb(153,153,153);
}
.zhuboxinxi .zhubo-form input[type="radio"]:checked{
	background-color: red;
}
.zhuboxinxi .pingtai{
	position: relative;
	top: 0;
	left: 0;
}
.zhuboxinxi .zhubo-form .pingtai input[type="radio"]{		
	cursor: pointer;
	width: 52px;
	height: 27px;
	appearance: none; 
	-webkit-appearance: none;
	outline: none;
	margin: 0;
	opacity: 0;
}
.zhuboxinxi .zhubo-form .pingtai input[type="radio"]:nth-child(1){
	position: absolute;
	top:0;
	left: 5px;
}
.zhuboxinxi .zhubo-form .pingtai input[type="radio"]:nth-child(3){
	position: absolute;
	top: 0;
	left: 60px;
}
.zhuboxinxi .zhubo-form .pingtai input[type="radio"]:nth-child(5){
	position: absolute;
	top: 0;
	left: 110px;
}
.zhuboxinxi .zhubo-form .pingtai input[type="radio"] + span{
	padding: 0px 6px;
	border-radius: 13px;
	border: 0.5px solid rgb(204,204,204);
	text-align: center;
	display: inline-block;
	margin-top: -3px;
	margin-left: 10px;
	color: #999999;
	}
.zhuboxinxi .zhubo-form .pingtai input[type="radio"]:checked + span{
	background-color: #303030;
	color: white;
	text-align: center;
	}
.zhuboxinxi .zhubo-form textarea{
		border: none;
		margin: 0;
		padding: 0;
		display: inline-block;
		font-size: 14px;
	}
.zhuboxinxi .zhubo-form .errormessage{
	text-align: center;
}
.zhuboxinxi .zhubo-form #error-message{
	color:red;
}
.zhuboxinxi .zhubo-form #error-message .icon{
	color: red;
}
/*public.css end*/


